{
  "name": "@traceway/logger",
  "version": "0.1.0",
  "description": "前端日志库 - 错误追踪、用户操作链路、性能监控",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "test:run": "vitest --passWithNoTests --run",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "logger",
    "error-tracking",
    "breadcrumbs",
    "performance",
    "monitoring"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "jsdom": "^24.1.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "web-vitals": "^3.5.0"
  },
  "peerDependencies": {
    "web-vitals": "^3.0.0"
  }
}
